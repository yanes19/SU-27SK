        <mp-systems>
            <radar-enabled type="bool">true</radar-enabled>
            <ecm-rwr-enabled type="bool">true</ecm-rwr-enabled>
            <combat-enabled type="bool">true</combat-enabled>
        </mp-systems>

        <chase-distance-m type="double" archive="y">-30.0</chase-distance-m>

        <!-- Flap settings -->
        <flaps>
            <setting>0.00</setting>
            <setting>1.00</setting>
        </flaps>

        <!-- TFA popup dialogue -->
        <gui>
            <dialogs>
                <TFA-popup>
                    <config>
                        <dialog include="Dialogs/TFA-popup.xml" />
                    </config>
                </TFA-popup>
            </dialogs>
        </gui>

        <!-- AoA Indexer target 7.0 deg -->
        <aoa-indexer>
            <too-slow-deg>8.5</too-slow-deg>  <!-- target +1.5 deg -->
            <slow-deg>7.5</slow-deg>          <!-- target +0.5 deg -->
            <fast-deg>6.5</fast-deg>          <!-- target -0.5 deg -->
            <too-fast-deg>-5.5</too-fast-deg>  <!-- target -1.5 deg -->
        </aoa-indexer>

        <!-- Help -->
        <help>
            <title>SU-27</title>
            <line>Aircraft spawns with "Operational" fuel values = 50% of maximum.</line>
            <line></line>
            <line>Cruise speed: 870 mph (1,400 km/h) ( 750kts ) at 32,780 ft (10,000m) </line>
            <line>Max LevelSpeed at altitude: 1,490 mph (2,400 km/h)at 32,780 ft (10,000 m), Mach
                2.3</line>
            <line>FCS working at 80% efficienty on this aircraft.</line>
            <line></line>
            <line> </line>
            <line></line>
            <line> </line>
            <key>
                <name>}</name>
                <desc>Startup/Shut down engines</desc>
            </key>
            <key>
                <name>Ctrl-D</name>
                <desc>Toggle DirectInput Mode (FBW Disengengage)</desc>
            </key>

            <key>
                <name>Ctrl-Space</name>
                <desc>Engage AP auto-leveller</desc>
            </key>

            <key>
                <name>e</name>
                <desc>Fire selected WCS weapon</desc>
                <binding>
                    <command>nasal</command>
                    <script>guns.fire_MG(1);</script>
                </binding>
            </key>

            <key>
                <name>m</name>
                <desc>Cycle selected WCS weapon</desc>

            </key>

            <key>
                <name>q</name>
                <desc>Release Flares</desc>
            </key>
            <key>
                <name>r</name>
                <desc>Switch Radar Range</desc>
            </key>
            <key>
                <name>w</name>
                <desc>Cycle Gun/Missile</desc>
            </key>
            <key>
                <name>y</name>
                <desc>Radar: select target</desc>
            </key>
            <key>
                <name>L</name>
                <desc>Toggle Nav lights</desc>
            </key>
            <key>
                <name>P</name>
                <desc>Parachute release</desc>
            </key>

            <key>
                <name>c</name>
                <desc>Canopy open/close</desc>
            </key>


            <key>
                <name>l</name>
                <desc>Toggle flashing beacons lights</desc>
            </key>

            <!--
        <key n="70">
        <name>F</name>
