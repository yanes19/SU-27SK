                <desc>Toggle Radar On/Off</desc>
            </key>

            <key>
                <name>Z</name>
                <desc>Reset view distance</desc>
                <repeatable type="bool">false</repeatable>
            </key>

            <key>
                <name>z</name>
                <desc>Increase view distance</desc>
            </key>


            <key>
                <name>J</name>
                <desc>Jettison Drag Parachute</desc>
            </key>
            <key>
                <name>j</name>
                <desc>Deploy Drag Parachute</desc>
            </key>
        </help>


