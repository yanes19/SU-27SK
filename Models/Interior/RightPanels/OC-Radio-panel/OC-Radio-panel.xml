<?xml version="1.0"?>


<PropertyList>
	
	<name>OC-Radio-panel</name>
	<path>OC-Radio-panel.ac</path>
	
    <animation>
		<type>knob</type>
		<object-name>knob.program.selector</object-name>
	  <property>su-27/instrumentation/PNK-10/active-mode</property>
	  <factor>30</factor>
		<center>
			<x-m>0.00786</x-m><y-m>0.05582</y-m><z-m>0.01863</z-m>
		</center>
		<axis>
			<x>0</x><y>0</y><z>1</z>
		</axis>
		<action>
			<binding>
			  <command>property-adjust</command>
			  <property>su-27/instrumentation/PNK-10/active-mode</property>
				  <factor>1</factor>
	              <min>0</min>
	              <max>4</max>                
	              <wrap>false</wrap>
			</binding>
		</action>
		<action>
			<binding>
				<condition>
				<not-equals>
					<property>/autopilot/locks/heading</property>
					<value />
				</not-equals>
				</condition>
	      <command>nasal</command>
	      <script>ACS.SAU_AutoLevel();</script>
			</binding>
		</action>
	</animation>	
	
	
	
