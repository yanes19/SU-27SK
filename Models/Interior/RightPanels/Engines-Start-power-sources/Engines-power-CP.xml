  <animation>	<!--External Power switch lever rotation-->
	<type>rotate</type>
	<object-name>Ground-power-Lever</object-name>
	<property>controls/electric/external-power</property>
	<factor>-60</factor>
	<offset-deg>30</offset-deg>
	  <center>
	   <x-m>-0.037277</x-m>
	   <y-m> 0.084487</y-m>
	   <z-m> 0.000 m</z-m>
	  </center>	   
	  <axis>
