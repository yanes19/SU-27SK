<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <name>AfterBurnerLeds</name>
    <path>AfterBurnerLeds.ac</path>
    <animation>
        <type>select</type>
        <object-name>ENG1.AfterBurner.led.lit</object-name>
        <condition>
            <not>
                <equals>
                    <property>engines/engine[0]/augmentation</property>
                    <value>0</value>
                </equals>
            </not>
        </condition>
    </animation>

    <animation>
        <type>material</type>
        <object-name>ENG1.AfterBurner.led.lit</object-name>
