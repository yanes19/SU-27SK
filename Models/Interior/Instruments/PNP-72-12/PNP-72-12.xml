<?xml version="1.0"?>

<PropertyList>

<path>PNP-72-12.ac</path>

<animation>
 <type>material</type>
 <object-name>PNP.CourseDisc</object-name>
 <object-name>PNP.BlenkerNID</object-name>
 <object-name>PNP.BlenkerNIU</object-name>
 <object-name>PNP.CN1</object-name>
 <object-name>PNP.CN2</object-name>
 <object-name>PNP.CN3</object-name>
 <object-name>PNP.CN4</object-name>
 <object-name>PNP.CN5</object-name>
 <object-name>PNP.CN6</object-name>
 <object-name>CurrentCourseMarker</object-name>
 <object-name>UpFace</object-name>
 <object-name>PNP.BlenkerK</object-name>
 <object-name>PNP.BlenkerG</object-name>
 <object-name>PNP.BlenkerKS</object-name>
 <object-name>PNP.CourseDisc</object-name>
 <object-name>PNP.IDCourse</object-name>
 <object-name>PNP.NeedleCourseAngle</object-name>
 <object-name>PNP.NCAngleRC</object-name>
 <object-name>PNP.NSetPathAngle</object-name>
 <object-name>ScaleDCGRM</object-name>
 <object-name>PlaneDCRM</object-name>
 <object-name>PlaneDGRM</object-name>
 <object-name>PNP.NCAngleRC</object-name>
 <object-name>PNP.AngleDerivIndex</object-name>
	 <condition>
		 <greater-than>
			 <property>systems/electrical/outputs/WarnLeds</property>
		   <value>23</value>
		 </greater-than>
	 </condition>
 <emission>
   <red>0.8</red>
   <green>0.5</green>
   <blue>0.5</blue>
  </emission>
</animation>

<!--
	<animation>
	 <type>select</type> 
	 <object-name>glass</object-name>
	 <condition>
	   <equals>
	   <property>constants/zero</property>
	   <value>1</value>
	   </equals>
	 </condition>
	</animation>
-->
<animation>
 <type>material</type>
 <object-name>glass</object-name>
  <transparency>
   <alpha-prop>constants/one</alpha-prop>
   <factor>0.30</factor>
  </transparency>
</animation>

 <animation>
  <type>textranslate</type>
  <object-name>glass</object-name>
  <property>sim/current-view/pitch-offset-deg</property>
  <factor>0.3</factor>
  <axis>
   <x> 0</x>
   <y>-1</y>
   <z> 0</z>
  </axis>
 </animation>

 <animation>
  <type>shader</type>
  <shader>chrome</shader>
  <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
  <object-name>glass</object-name>
  <object-name>PNP.BlenkerNID</object-name>
  <object-name>PNP.BlenkerNIU</object-name>
 </animation>
 


<animation>
 <type>rotate</type>
 <object-name>PNP.CourseDisc</object-name>
 <property>orientation/heading-deg</property>
 <center>
  <x-m> 0.0</x-m>
  <y-m> 0.0</y-m>
  <z-m>-0.00850002</z-m>
 </center>
 <axis>
  <x>1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>PNP.NSetPathAngle</object-name>
 <property>su-27/instrumentation/HSI/SelectedCourseDirector</property>
 <center>
  <x-m> 0.0</x-m>
  <y-m> 0.0</y-m>
  <z-m>-0.00850002</z-m>
 </center>
 <axis>
  <x>-1</x>
  <y>0</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>PNP.NCAngleRC</object-name>
   <property>su-27/instrumentation/HSI/IndicatedTrgtourse</property>
 <center>
  <x-m> 0.0</x-m>
  <y-m> 0.0</y-m>
  <z-m>-0.00850002</z-m>
 </center>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>PNP.CN1</object-name>
 <property>instrumentation/dme/indicated-distance-nm</property>
 <factor>0.36</factor>
 <factor>36</factor>
 <center>
  <x-m>-0.009</x-m>
  <y-m> 0.000</y-m>
  <z-m> 0.045</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>PNP.CN2</object-name>
 <property>instrumentation/dme/indicated-distance-nm</property>
 <factor>3.6</factor>
 <step>36</step>
 <center>
  <x-m>-0.009</x-m>
  <y-m> 0.000</y-m>
  <z-m> 0.045</z-m>
  <!--z-m>-0.00850002</z-m-->
 </center>
 <axis>
  <x>0</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>PNP.CN3</object-name>
 <property>instrumentation/dme/indicated-distance-nm</property>
 <factor>36</factor>
 <step>36</step>
 <center>
  <x-m>-0.009</x-m>
  <y-m> 0.000</y-m>
  <z-m> 0.045</z-m>
 </center>
 <axis>
  <x>0</x>
  <y>-1</y>
  <z>0</z>
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>PNP.CN4</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
 <factor>0.001</factor>
 <step>100</step>
 <axis>
  <x> 0</x>
  <y>-1</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>PNP.CN5</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
 <factor>0.01</factor>
 <step>10</step>
 <axis>
  <x> 0</x>
  <y>-1</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>textranslate</type>
 <object-name>PNP.CN6</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
 <factor>0.1</factor>
 <step>1</step>
 <axis>
  <x> 0</x>
  <y>-1</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>translate</type>
 <object-name>GS.Needle</object-name>
 <property>instrumentation/nav/gs-needle-deflection-norm</property>
 <factor>0.007</factor>
 <axis>
  <x>0</x>
  <y>0</y>
  <z>1</z>
 </axis>
</animation>

<animation>
 <type>translate</type>
 <object-name>ILS.HDG.Needle</object-name>
  <condition>
	<and>
  <greater-than>
   <property>instrumentation/nav/crosstrack-error-m</property>
   <value>-2500</value>
  </greater-than>

  <less-than>
   <property>instrumentation/nav/crosstrack-error-m</property>
   <value>2500</value>
  </less-than>
  </and>
 </condition>
 <property>instrumentation/nav/crosstrack-error-m</property>
	<factor>0.000016</factor>
	<min>-0.0010</min>
	<max> 0.0010</max>
	<axis>
	<x>0</x>
	<y>1</y>
	<z>0</z>
	</axis>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerK</object-name>
 <condition>
  <equals>
   <property>mig29/o/snv</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerG</object-name>
 <condition>
  <equals>
   <property>mig29/o/snv</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerKS</object-name>
 <condition>
  <equals>
   <property>fdm/jsbsim/systems/IK-VK/IK-VK-on</property>
   <value>0</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerNID</object-name>
 <condition>
  <equals>
   <property>mig29/o/snv</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>select</type>
 <object-name>PNP.BlenkerNIU</object-name>
 <condition>
  <equals>
   <property>mig29/o/snv</property>
   <value>1</value>
  </equals>
 </condition>
</animation>

<animation>
 <type>translate</type>
 <object-name>PNP.Button</object-name>
 <property>mig29/controls/PNP-72-12/test</property>
 <factor>0.003</factor>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>


<animation>
	<type>knob</type>
	<object-name>PNP.knobSetPathAngle</object-name>
  <property>instrumentation/nav/radials/selected-deg</property>
  <factor>1.0</factor>
	<center>
		<x-m>0.00936</x-m><y-m>0.044</y-m><z-m>-0.0482</z-m>
	</center>
	<axis>
		<x>1</x><y>0</y><z>0.0</z>
	</axis>
	<action>
		<binding>
		  <command>property-adjust</command>
		  <property>instrumentation/nav/radials/selected-deg</property>
		  <factor>1</factor>
              <min>0</min>
              <max>360</max>                
              <wrap>true</wrap>
		</binding>
	</action>

</animation>	


<animation>
 <type>rotate</type>
 <object-name>PNP.knobSetPathAngle</object-name>
 <property>instrumentation/nav/radials/selected-deg</property>
 <factor>5</factor>
 <center>
  <x-m> 0.0000</x-m>
  <y-m> 0.0440</y-m>
  <z-m>-0.0482</z-m>
 </center>
 <axis>
  <x>-1</x>
  <y> 0</y>
  <z> 0</z>
 </axis>
</animation>

<animation>
 <type>pick</type>
 <object-name>PNP.cz1</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>false</repeatable>
  <binding>
   <command>property-assign</command>
   <property>mig29/controls/PNP-72-12/test</property>
   <value>1</value>
  </binding>
  <mod-up>
   <binding>
    <command>property-assign</command>
    <property>mig29/controls/PNP-72-12/test</property>
    <value>0</value>
   </binding>
  </mod-up>
 </action>
</animation>

<animation>
 <type>pick</type>
 <object-name>PNP.cz2</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>true</repeatable>
  <binding>
   <command>property-adjust</command>
   <property>instrumentation/nav/radials/selected-deg</property>
   <step>-1</step>
   <min>0</min>
   <max>359</max>
  </binding>
 </action>
</animation>
<animation>
 <type>pick</type>
 <object-name>PNP.cz3</object-name>
 <visible>false</visible>
 <action>
  <button>0</button>
  <repeatable>true</repeatable>
  <binding>
   <command>property-adjust</command>
   <property>instrumentation/nav/radials/selected-deg</property>
   <step>1</step>
   <min>0</min>
   <max>359</max>
  </binding>
 </action>
</animation>

</PropertyList>
