 <!--++++++++++++ target0 air to air ++++++++++-->
 <animation>
	 <type>select</type>
	 <object-name>target0</object-name>
	 <condition>   
		<and>
			<equals>
				<property>instrumentation/radar2/targets/aircraft/display</property>
				<value>1</value>
			</equals>
			<equals>
				<property>instrumentation/radar/selected</property>  
				<value>2</value>
			</equals>
			<equals>
				<property>su-27/instrumentation/N010-radar/emitting</property>  
				<value>1</value>
			</equals>
	  </and>
	 </condition>
 </animation>

 <animation>
	 <type>translate</type>
	 <object-name>target0</object-name>
	 <property>instrumentation/radar2/targets/aircraft/y-shift</property>
<!--
	 <factor>0.001888</factor>
-->
	 <axis>
		 <x>0</x>
		 <y>0</y>
		 <z>1</z>
	 </axis>
 </animation>
 <animation>
	 <type>translate</type>
	 <object-name>target0</object-name>
	 <property>instrumentation/radar2/targets/aircraft/x-shift</property>
<!--
		<factor>0.001888</factor>
-->
	 <axis>
		 <x>0</x>
		 <y>1</y>
		 <z>0</z>
	 </axis>
 </animation>
 <animation>
	 <type>rotate</type>
	 <object-name>target0</object-name>
	 <property>instrumentation/radar2/targets/aircraft/rotation</property>
	 <axis>
		 <x>-1</x>
		 <y>0</y>
		 <z>0</z>
	 </axis>
	 <center>
		 <x-m>0.00</x-m>
		 <y-m>0.0050</y-m>
		 <z-m>-0.0340</z-m>
