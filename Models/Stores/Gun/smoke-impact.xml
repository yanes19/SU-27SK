<?xml version="1.0"?>

<PropertyList>
	<path>puff-impact.ac</path>

	<animation>
		<type>billboard</type>
		<spherical>true</spherical>
	</animation>

	<animation>
		<type>scale</type>
		<object-name>puff</object-name>
		<use-personality>true</use-personality>
		<property>sim/time/elapsed-sec</property>
		<x-min>0.3</x-min>
		<y-min>0.3</y-min>
		<z-min>0.3</z-min>
		<x-factor>0.05</x-factor>
		<y-factor>0.05</y-factor>
		<z-factor>0.05</z-factor>
	</animation>

	<animation>
		<type>blend</type>
		<object-name>puff</object-name>
		<use-personality>true</use-personality>
		<property>sim/time/elapsed-sec</property>
		<factor>0.035</factor>
		<min>0.05</min>
		<max>1.0</max>
	</animation>

	<animation>
		<type>dist-scale</type>
		<object-name>disk</object-name>
		<use-personality>true</use-personality>
		<interpolation>
			<entry><ind>0</ind><dep>1</dep></entry>
			<entry><ind>500</ind><dep>2</dep></entry>
			<entry><ind>1000</ind><dep>4</dep></entry>
			<entry><ind>3000</ind><dep>10</dep></entry>
		</interpolation>
	</animation>

	<animation>
		<type>blend</type>
		<object-name>disk</object-name>
		<use-personality>true</use-personality>
		<property>sim/time/elapsed-sec</property>
		<factor>3</factor>
		<min>0.05</min>
		<max>1.0</max>
	</animation>

	<animation>
		<type>material</type>
		<object-name>puff</object-name>
		<use-personality>true</use-personality>
		<emission>
			<red>0.60</red>
			<green>0.39</green>
			<blue>0.2</blue>
		</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>disk</object-name>
		<use-personality>true</use-personality>
		<emission>
			<red>1</red>
			<green>1</green>
			<blue>0.6</blue>
		</emission>
	</animation>

</PropertyList>
