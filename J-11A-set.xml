        <multiplay>
            <chat_display>1</chat_display>
            <generic>
                <!-- Nav lights on -->
                <int n="0" alias="/controls/lighting/nav-lights-switch"></int>
                <!-- Beacon on -->
                <int n="1" alias="/controls/lighting/beacon-switch"></int>
                <!-- Landing lights on -->
                <int n="2" alias="/controls/lighting/landing-lights"></int>
                <!-- Taxi light on -->
                <int n="3" alias="/controls/lighting/taxi-light-switch"></int>
